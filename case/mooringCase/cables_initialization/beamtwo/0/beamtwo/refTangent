FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceVectorField;
    location    "0/beamtwo";
    object      refTangent;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];


internalField   uniform (-0.883945 -0.297609 -0.360654);

boundaryField
{
    left
    {
        type            calculated;
        value           uniform (-0.883945 -0.297609 -0.360654);
    }
    right
    {
        type            calculated;
        value           uniform (-0.883945 -0.297609 -0.360654);
    }
    beam
    {
        type            empty;
        value           nonuniform List<vector> 0();
    }
}


