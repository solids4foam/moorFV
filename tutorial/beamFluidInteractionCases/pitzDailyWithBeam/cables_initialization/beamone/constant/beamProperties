|   \\  /    A nd           | Web:         http://www.foam-extend.org         |
|    \\/     M anipulation  |                                                 |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      beamProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

beamModel coupledTotalLagNewtonRaphsonBeam;

nBeams 1;
R R [0 1 0 0 0 0 0] 4E-04;
L L [0 1 0 0 0 0 0] 0.0508; 
nSegments 60; 


"coupledTotalLagNewtonRaphsonBeamCoeffs"
{
    E E [1 -1 -2 0 0 0 0] 1825834.598604678;;
    G G [1 -1 -2 0 0 0 0] 912917.299302339;
    rho rho [1 -3 0 0 0 0 0] 2000;

    rhoFluid rhoFluid [1 -3 0 0 0 0 0] 1.2;

    groundContactActive false;
    groundZ -0.5;
    gStiffness 1E03;
    gDamping 1.0;
    nSegments 60;

    startPatchName left;
    endPatchName right;

    nCorrectors 200;
    convergenceTol 1e-10;

    debug yes;

    updatedLagrangian no;

    //   ibrahimovicCase true;

    objectiveInterpolation false;

    steadyState false;

    // Newmark-beta integration scheme
    newmark false;

    // Flag to activate drag contribution                                                                                                                                                                                            
    // steadyState flag should also be false along with this flag                                                                                                                                                                    
    // else drag forces will be zero                                                                                                                                                                                                 
    dragActive true;
    beamFluidInteraction true;
    // this changes how to determine cell marker for each cell                                                                                                                                                                       
    // true : uses fractional Marking i.e: numberOfPointsInside/totalNumberOfPoints                                                                                                                                                  
    // false : determines the values based on the location of cell centres (0 or 1)                                                                                                                                                  
    fractionalMarking false;

    
    // Coefficient of drag in normal and tangential directions
    // Default value if not specified is 1.0 for both
    Cdn 1.6;
    Cdt 0.05;
    CMn 1.0;
    CMt 0.0;
}



// ************************************************************************* //
