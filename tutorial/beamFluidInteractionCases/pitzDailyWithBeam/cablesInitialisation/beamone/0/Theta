FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DTheta;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type    momentBeamRotationNR;
        moment  uniform (0 0 0);
        value   uniform (0 0 0);
    }

    right
    {
        type    momentBeamRotationNR;
        moment  uniform (0 0 0);
        value   uniform (0 0 0);
    }

    beam
    {
        type empty;
    }
}


