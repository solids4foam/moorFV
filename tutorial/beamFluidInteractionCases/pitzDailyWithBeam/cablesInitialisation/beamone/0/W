FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      DW;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type    fixedValue;
        value   uniform (0 0 0);
    }

    right
    {
        type    fixedDisplacement;
        displacementSeries
        {
            file        "$FOAM_CASE/constant/timeVsDisplacement";
            outOfBounds     clamp;
        }
        value   uniform (0 0 0);
    }

    beam
    {
        type    empty;
    }
}


